<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vn.aptech.baikiemtra.controller.Bai3Controller">
   <children>
      <AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Circle fx:id="shapeCircle" fill="#fff01f" layoutX="157.0" layoutY="119.0" radius="58.0" stroke="BLACK" strokeType="INSIDE" />
            <Rectangle fx:id="shapeRect" arcHeight="5.0" arcWidth="5.0" fill="#ff1f1f" height="104.0" layoutX="215.0" layoutY="248.0" stroke="BLACK" strokeType="INSIDE" width="103.0" />
            <Ellipse fx:id="shapeElipse" fill="DODGERBLUE" layoutX="414.0" layoutY="119.0" radiusX="96.0" radiusY="49.0" stroke="BLACK" strokeType="INSIDE" />
         </children>
      </AnchorPane>
      <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="menuClearAll" mnemonicParsing="false" onAction="#clearAll" text="Clear All" />
                  <MenuItem fx:id="menuBackToMainMenu" mnemonicParsing="false" onAction="#switchToMenu" text="Back to Main Menu" />
                  <MenuItem fx:id="menuExit" mnemonicParsing="false" onAction="#exit" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Tool">
            <items>
              <MenuItem fx:id="menuOption" mnemonicParsing="false" onAction="#openOption" text="Option" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
